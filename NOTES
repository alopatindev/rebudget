UI:
    outcome filter
        sms address
        text contains
        cost integer regexp
        cost frac regexp
        store
        category

    income filter
        sms address
        text contains
        cost integer regexp
        cost frac regexp


DB:
    filters
        int id
        bool outcome
        string sms_address
        string sms_text_contains
        string cost_integer_regexp
        string cost_frac_regexp
        string remaining_integer_regexp
        string remaining_frac_regexp
        string store_regexp

    categories
        int id
        string name
        int color
        float money_planned
        float money_spent

    wallets
        int id
        string name
        float money

    stats
        int id
        int datetime
        int category_id
        float planned
        float spent

    stores
        int id
        string name
        int category_id
